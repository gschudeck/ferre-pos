{
  "app": {
    "name": "smtp-client",
    "version": "1.0.0",
    "environment": "development"
  },
  "logging": {
    "level": "info",
    "format": "json"
  },
  "providers": {
    "gmail": {
      "smtp_host": "smtp.gmail.com",
      "smtp_port": 587,
      "use_tls": true,
      "timeout": "30s"
    },
    "office365": {
      "smtp_host": "smtp.office365.com", 
      "smtp_port": 587,
      "use_tls": true,
      "timeout": "30s"
    }
  }
}